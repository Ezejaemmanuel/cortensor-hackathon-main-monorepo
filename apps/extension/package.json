{
  "name": "extension",
  "description": "A modern browser extension template with sidepanel support, built with WXT + Tailwind CSS 4.0 + shadcn/ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:chrome": "wxt -b chrome",
    "dev:firefox": "wxt -b firefox",
    "dev:edge": "wxt -b edge",
    "dev:safari": "wxt -b safari",
    "build": "wxt build",
    "build:chrome": "wxt build -b chrome",
    "build:firefox": "wxt build -b firefox",
    "build:edge": "wxt build -b edge",
    "build:safari": "wxt build -b safari",
    "zip": "wxt zip",
    "zip:chrome": "wxt zip -b chrome",
    "zip:firefox": "wxt zip -b firefox",
    "zip:edge": "wxt zip -b edge",
    "zip:safari": "wxt zip -b safari",
    "compile": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@rollup/rollup-win32-x64-msvc": "^4.46.4",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.85.3",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.462.0",
    "postcss": "^8.4.49",
    "radix-ui": "^1.4.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.4",
    "viem": "2.x",
    "wagmi": "^2.16.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^24.0.7",
    "@wxt-dev/module-react": "^1.1.3",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wxt": "^0.20.6"
  },
  "packageManager": "pnpm@9.10.0"
}
